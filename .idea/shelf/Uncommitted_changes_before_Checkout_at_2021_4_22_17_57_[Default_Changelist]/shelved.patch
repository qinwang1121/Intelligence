+package com.qinwang.intelligence.tools;
+
+import android.view.View;
+import android.widget.TextView;
+
+import androidx.annotation.NonNull;
+
+import com.qinwang.intelligence.R;
+import com.qinwang.intelligence.tools.bean.RoadMsg;
+import com.zhpan.bannerview.BaseViewHolder;
+
+/**
+ * @Auther:haoyanwang1121@gmail.com
+ * @Date:2021/4/22
+ * @Description:com.qinwang.intelligence.tools
+ * @Version:1.0
+ * @function:
+ */
+public class ListViewHolder extends BaseViewHolder<RoadMsg> {
+
+    public ListViewHolder(@NonNull View itemView) {
+        super(itemView);
+    }
+
+    @Override
+    public void bindData(RoadMsg data, int position, int pageSize) {
+        TextView roadName, roadEva, roadSpeed, roadDis, roadTrend, roadDes;
+        roadName = (TextView) findView(R.id.banner_roadName);
+        roadEva = (TextView) findView(R.id.banner_roadEva);
+        roadSpeed = (TextView) findView(R.id.banner_roadSpeed);
+        roadDis = (TextView) findView(R.id.banner_roadDis);
+        roadTrend = (TextView) findView(R.id.banner_roadTrend);
+        roadDes = (TextView) findView(R.id.banner_roadDes);
+
+        roadName.setText(data.getRoad_name());
+        roadEva.setText(data.getStatus());
+        roadSpeed.setText(String.valueOf(data.getSpeed()));
+        roadDis.setText(String.valueOf(data.getCongestion_distance()));
+        roadTrend.setText(data.getCongestion_trend());
+        roadDes.setText(data.getSection_desc());
+    }
+}
